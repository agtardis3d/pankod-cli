    "url": "https://github.com/pankod/pankod-cli/issues"
  },
  "homepage": "https://github.com/pankod/pankod-cli",
  "files": [
    "dist"
  ],
  "keywords": [
    "pankod",
    "cli",
    "typescript",
    "moleculer",
    "nextjs",
    "react-native"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "figlet": "^1.2.1",
    "inquirer": "^6.3.1",
    "log-symbols": "^3.0.0",
    "mustache": "^3.0.1"
  },
