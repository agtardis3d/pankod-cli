{
  "name": "@pankod/pankod-cli",
  "version": "0.1.3",
  "description": "CLI for Pankod open source projects.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run sync-templates",
    "test": "echo \"Error: no test specified\" && exit 1",
